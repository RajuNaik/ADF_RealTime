{
	"name": "dataflow_src_stg",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_src_file",
						"type": "DatasetReference"
					},
					"name": "sourceff"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "alterRow1"
				},
				{
					"name": "sort1"
				},
				{
					"name": "sort2"
				}
			],
			"scriptLines": [
				"source(output(",
				"          Column_1 as string,",
				"          Column_2 as string,",
				"          Column_3 as string,",
				"          Column_4 as string,",
				"          Column_5 as string,",
				"          Column_6 as string,",
				"          Column_7 as string,",
				"          Column_8 as string,",
				"          Column_9 as string,",
				"          Column_10 as string",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceff",
				"sort1 alterRow() ~> alterRow1",
				"sort2 sort() ~> sort1",
				"sourceff sort() ~> sort2"
			]
		}
	}
}