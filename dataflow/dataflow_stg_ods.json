{
	"name": "dataflow_stg_ods",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_stg_sql_db",
						"type": "DatasetReference"
					},
					"name": "source"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_ods_sql_db",
						"type": "DatasetReference"
					},
					"name": "sink"
				}
			],
			"transformations": [
				{
					"name": "filter"
				},
				{
					"name": "derivedColumn"
				}
			],
			"scriptLines": [
				"source(output(",
				"          caseid as integer,",
				"          country as string,",
				"          country_code as string,",
				"          continent as string,",
				"          population as string,",
				"          indicator as string,",
				"          daily_count as string,",
				"          date as date,",
				"          rate_14_day as string,",
				"          source as string,",
				"          insert_ts as date",
				"     ),",
				"     allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     isolationLevel: 'READ_UNCOMMITTED',",
				"     format: 'table') ~> source",
				"source filter(country_code=='IND' || country_code=='GBR') ~> filter",
				"filter derive(country_concat = concat(country,'-',country_code),",
				"          insert_ts = currentDate()) ~> derivedColumn",
				"derivedColumn sink(allowSchemaDrift: false,",
				"     validateSchema: false,",
				"     input(",
				"          caseid as integer,",
				"          continent as string,",
				"          population as string,",
				"          indicator as string,",
				"          daily_count as string,",
				"          date as date,",
				"          rate_14_day as string,",
				"          source as string,",
				"          insert_ts as date,",
				"          country_concat as string",
				"     ),",
				"     deletable:false,",
				"     insertable:true,",
				"     updateable:false,",
				"     upsertable:false,",
				"     truncate:true,",
				"     format: 'table',",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     errorHandlingOption: 'stopOnFirstError',",
				"     mapColumn(",
				"          caseid,",
				"          country,",
				"          country_code,",
				"          continent,",
				"          population,",
				"          indicator,",
				"          daily_count,",
				"          date,",
				"          rate_14_day,",
				"          source",
				"     )) ~> sink"
			]
		}
	}
}